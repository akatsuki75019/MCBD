<div id="<%= dom_id book %>">
  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= book.description %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= book.quantity %>
  </p>

  <p>
    <strong>Release date:</strong>
    <%= book.release_date %>
  </p>

  <p>
    <strong>Tva:</strong>
    <%= book.tva %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= book.image_url %>
  </p>

</div>
